
    <div class="container">
        <h1>Tela de Consulta</h1>
        <table class="table">
           <thead>
                    <tr>
                        <th scope="col">Produto</th>
                        <th scope="col">Marca</th>
                        <th scope="col">Modelo </th>
                        <th scope="col">Descrição</th>
                        <th scope="col">Palavras Chaves</th>
                        <th scope="col">Ações</th>
                    </tr>
                </thead>
                <tbody >
                {{#each post}}
                    {{#dataValues}}
                    <tr>
                        <td>{{produto}}</td>
                        <td>{{marca}}</td>
                        <td>{{modelo}}</td>
                        <td>{{descricao}}</td>
                        <td>{{palavraschaves}}</td>
                        <td>
                    <td>
                        <button class="btn btn-primary"><a href="/editar/{{id}}">Editar</a></button>
                        <button class="btn btn-danger"><a href="/excluir/{{id}}">Excluir</a></button>
                     </td>
                    </tr>
                    {{/dataValues}}
                {{/each}}
                </tbody>
            </table>
    </div>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
